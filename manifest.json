{
  "name": "Fancymetry",
  "description": "Make the free Israeli psychometric course fancy",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://x.psychometrix.co.il/*"],
      "css": ["src/x-psychometrix.css"],
      "js": ["src/x-psychometrix.js"]
    },
    {
      "matches": [
        "https://courses.campus.gov.il/courses/course-v1:MSE+GOV_PsychometryHe+2018_1/*"
      ],
      "css": ["src/campus-il.css"],
      "js": ["src/campus-il.js"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
